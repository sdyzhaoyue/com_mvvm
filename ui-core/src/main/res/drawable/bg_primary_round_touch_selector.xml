<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dimen_line" android:color="@color/uiColorGray" />
            <solid android:color="@color/uiColorGray" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dimen_line" android:color="@color/uiColorLine" />
            <corners android:radius="@dimen/dimen_smaller" />
            <solid android:color="@color/uiTouchRipple" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dimen_line" android:color="@color/uiColorLine" />
            <corners android:radius="@dimen/dimen_smaller" />
            <solid android:color="@color/uiColorPrimaryDark" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dimen_line" android:color="@color/uiColorLine" />
            <corners android:radius="@dimen/dimen_smaller" />
            <solid android:color="@color/uiColorPrimaryAccent" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dimen_line" android:color="@color/uiColorLine" />
            <corners android:radius="@dimen/dimen_smaller" />
            <solid android:color="@color/uiColorPrimary" />
        </shape>
    </item>
</selector>
