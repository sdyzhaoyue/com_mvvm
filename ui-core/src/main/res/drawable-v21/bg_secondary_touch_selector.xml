<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/uiColorSecondary">
    <item>
        <selector>
            <item
                android:drawable="@color/uiTouchRipple"
                android:state_pressed="true" />
            <item
                android:drawable="@color/uiColorSecondaryDark"
                android:state_focused="true" />
            <item
                android:drawable="@color/uiColorSecondaryAccent"
                android:state_checked="true" />
            <item android:drawable="@color/uiColorSecondary" />
        </selector>
    </item>
</ripple>