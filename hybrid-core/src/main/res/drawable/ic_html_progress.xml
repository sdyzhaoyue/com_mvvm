<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2dp"/>
            <gradient
                android:angle="270"
                android:centerColor="@color/uiColorWhite"
                android:endColor="@color/uiColorWhite"
                android:startColor="@color/uiColorWhite"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2dp"/>
                <gradient
                    android:centerColor="@color/uiColorPrimary"
                    android:endColor="@color/uiColorPrimary"
                    android:startColor="@color/uiColorPrimary"/>

            </shape>
        </clip>
    </item>

</layer-list>